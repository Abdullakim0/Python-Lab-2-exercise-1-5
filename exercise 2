sorted_alphabetically = sorted(inscriptions)
sorted_by_length = sorted(inscriptions, key=len, reverse=True)
sorted_by_vowels = sorted(inscriptions, key=lambda x: sum(1 for char in x if char.lower() in 'aeiou'))
sorted_by_unique_letters = sorted(inscriptions, key=lambda x: len(set(x)))
sorted_by_a_first = sorted(inscriptions, key=lambda x: ('a' in x, x))
